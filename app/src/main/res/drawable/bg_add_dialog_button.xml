<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">

    <item android:state_pressed="true">
        <shape>
            <corners android:radius="10dp"/>
            <stroke android:color="@color/circleEdgeLineClicked"/>
            <solid android:color="@color/circleMainClicked"/>
        </shape>
    </item>
    <item>
        <shape>
            <corners android:radius="10dp"/>
            <solid android:color="@color/circleMain"/>
            <stroke android:color="@color/circleEdgeLine"/>
        </shape>
    </item>
</selector>